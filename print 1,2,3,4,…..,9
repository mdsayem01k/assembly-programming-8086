;Write a program that can print 1,2,3,4,â€¦..,9


.MODEL SMALL 
.STACK 100H
.DATA
.CODE
MAIN PROC
    MOV CX,9
    
    MOV AH,2
    MOV DL,49
    
    LABEL:
    INT 21H  
    MOV DH,DL
    
    MOV DL,32    ;SPACE
    MOV AH,2
    INT 21H
    
    MOV DL,DH
    INC DL 
    LOOP LABEL
    
    
    MOV AH,4CH
    INT 21H
    MAIN ENDP   
END MAIN
